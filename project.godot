; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SevensEngine-GDT"
run/main_scene="res://world.tscn"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
run/max_fps=60
config/icon="res://icon.svg"

[audio]

driver/mix_rate=48000

[autoload]

Inventory="*res://scripts/inventory.gd"
EventManager="*res://quests/EventManager.gd"
EventList="*res://quests/EventList.gd"
QuestSystem="*res://quests/QuestSystem.gd"
Quest="*res://quests/Quest.gd"
QuestList="*res://quests/testq/QuestList.gd"
MusicManager="res://scripts/MusicManager.gd"
MusicManagerHandler="*res://sounds/BGM/dynmustest/music_manager_handler.tscn"
OveliaAnimHandler="*res://vn/chars/OveliaAnimHandler.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Ovelia": "res://vn/chars/Ovelia.dch",
"The Creepy Salesman": "res://vn/chars/The Creepy Salesman.dch",
"unit_test_character": "res://Tests/Resources/unit_test_character.dch"
}
directories/dtl_directory={
"dynmustest": "res://vn/timeleines/dynmustest.dtl",
"missotalk1": "res://vn/timeleines/missotalk1.dtl",
"missotalk1_tbrushcomplete": "res://vn/timeleines/missotalk1_tbrushcomplete.dtl",
"missotalk1_tbrushget": "res://vn/timeleines/missotalk1_tbrushget.dtl",
"sales_aftertbrush": "res://vn/timeleines/sales_aftertbrush.dtl",
"sales_toiletbrush": "res://vn/timeleines/sales_toiletbrush.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://vn/styles/mainstyle.tres"]
layout/default_style="res://vn/styles/mainstyle.tres"
variables={
"missotalk1": {
"no!": false,
"yes!": false
},
"sales_toiletbrush": {
"key_name": "",
"toiletbrush": false,
"whatyougot": false
}
}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
text/input_action="dialogic_default_action"
history/simple_history_enabled=true
history/already_read_history_enabled=true
timer/process_in_physics=true
text/advance_delay=0.1

[display]

window/size/mode=3
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="SevensEngine-GDT"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/func_godot/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg", "res://addons/import_scml/plugin.cfg")

